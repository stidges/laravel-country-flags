{
    "name": "stidges/laravel-country-flags",
    "description": "A Laravel wrapper for the stidges/country-flags package",
    "keywords": [
        "laravel",
        "country-flags",
        "countries",
        "country",
        "flag",
        "emoji",
        "unicode"
    ],
    "homepage": "https://github.com/stidges/laravel-country-flags",
    "license": "MIT",
    "authors": [
        {
            "name": "Stidges",
            "email": "info@stidges.com",
            "homepage": "https://stidges.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "stidges/country-flags": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Stidges\\LaravelCountryFlags\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Stidges\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stidges\\LaravelCountryFlags\\CountryFlagsServiceProvider"
            ],
            "aliases": {
                "CountryFlag": "Stidges\\LaravelCountryFlags\\CountryFlagFacade"
            }
        }
    }
}
